{
  "name": "czernika/wp-vite",
  "type": "package",
  "require": {
    "php": "^8.0",
    "netresearch/jsonmapper": "^4.1"
  },
  "require-dev": {
    "pestphp/pest": "^1.22"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Wolat\\": "src/core"
    }
  }
}
